<% itemId = `${centroId}${question._id}`%>

<textarea
  class="form-control"
  id="<%= itemId%>"
  name="<%= itemId%>"
  rows="3"
  centroId="<%=centroId%>"
  answerId="<%=question.ANSWER_ID ? question.ANSWER_ID:undefined %>"
  questionId="<%=question._id%>"
  onfocusout="updateFormItem(this)"
>
<%=question.ANSWER%>
</textarea>
