<select id="<%= question.ANSWER_ID%>" name="<%= question.ANSWER_ID%>" class="form-select" questionId="<%=question._id%>" onfocusout="updateFormItem(this)">
    <% question.PRESET_VALUES.forEach(function(option) { %>
    <option <%if(question.ANSWER == option){%> selected <%}%>><%= option %></option>
    <% }); %>
  </select>